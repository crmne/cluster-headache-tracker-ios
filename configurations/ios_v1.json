{
  "settings": {},
  "rules": [
    {
      "patterns": [
        "/headache_logs$",
        "/charts$",
        "/settings$",
        "/feedback$"
      ],
      "properties": {
        "context": "default",
        "presentation": "replace"
      }
    },
    {
      "patterns": [
        "/headache_logs/new$",
        "/headache_logs/.*/edit$"
      ],
      "properties": {
        "context": "modal",
        "pull_to_refresh_enabled": true,
        "presentation": "default",
        "modal_style": "pageSheet",
        "modal_dismiss_gesture_enabled": true
      }
    },
    {
      "patterns": [
        "/users/sign_in$",
        "/users/sign_up$",
        "/users/password/new$",
        "/users/password$",
        "/login$",
        "/signup$",
        "/register$"
      ],
      "properties": {
        "context": "modal",
        "pull_to_refresh_enabled": false,
        "presentation": "default",
        "modal_style": "pageSheet",
        "modal_dismiss_gesture_enabled": false
      }
    }
  ]
}
